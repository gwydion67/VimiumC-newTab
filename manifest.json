{
  "name" : "VimiumC-newTab",
  "author": "gwydion67",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "default_locale": "en",
  "description": "__MSG_description__",
  "homepage_url": "https://github.com/gwydion67/VimiumC-newTab",
  "icons": {
    "128": "./assets/icon128.png",
    "16": "./assets/icon16.png",
    "32": "./assets/icon32.png",
    "48": "./assets/icon48.png"
  },
  "incognito": "spanning",
  "manifest_version": 3,
  "minimum_chrome_version": "32",
  "options_page": "options.html",
  "permissions": [
		"bookmarks",
		"favicon",
		"topSites",
		"tabs",
		"fontSettings",
    "chrome://favicon/",
    "storage",
		"sessions"
	],
	"optional_host_permissions": [
		"file:///"
	],
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"options_ui": {
		"page": "newtab.html?options",
		"open_in_tab": true
	},
  "version": "0.1",
  "web_accessible_resources": [ "vomnibar*" ]
}
